    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericMin4Max8Type                                                       -->
  <!--================================================================================-->
  <xs:simpleType name="NumericMin4Max8Type">
    <xs:restriction base="NumType">
      <xs:pattern value="[0-9]{4,8}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithoutZero_1                                                      -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithoutZero_1">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="9" />
      <xs:pattern value="[1-9]" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithoutZero_3                                                      -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithoutZero_3">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="999" />
      <xs:pattern value="[1-9][0-9]{0,2}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithoutZero_5                                                      -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithoutZero_5">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="99999" />
      <xs:pattern value="[1-9][0-9]{0,4}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithoutZero_8                                                      -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithoutZero_8">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="99999999" />
      <xs:pattern value="[1-9][0-9]{0,7}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithZero_3                                                         -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithZero_3">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="999" />
      <xs:pattern value="[0-9]{1,3}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithZero_4                                                         -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithZero_4">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="9999" />
      <xs:pattern value="[0-9]{1,4}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithZero_8                                                         -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithZero_8">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="99999999" />
      <xs:pattern value="[0-9]{1,8}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithZero_9                                                         -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithZero_9">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="999999999" />
      <xs:pattern value="[0-9]{1,9}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumType                                                                   -->
  <!--================================================================================-->
  <xs:simpleType name="NumType">
    <xs:annotation>
      <xs:documentation>Base class for all nN and n..N
				types</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token" />
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== phaseIDtype                                                               -->
  <!--================================================================================-->
  <xs:simpleType name="phaseIDtype">
    <xs:restriction base="xs:token">
      <xs:enumeration value=" NCTS5.1">
        <xs:annotation>
          <xs:documentation>Phase NCTS 5.1</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="NCTS5.0">
        <xs:annotation>
          <xs:documentation>Phase NCTS 5.0</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== PostalCodeType                                                            -->
  <!--================================================================================-->
  <xs:simpleType name="PostalCodeType">
    <xs:annotation>
      <xs:documentation>Trader Postal Code (format: an..17)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:maxLength value="17" />
      <xs:pattern value=".{1,17}" />
    </xs:restriction>
    <!-- Updated -->
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== RiskAnalysisResultCode                                                    -->
  <!--================================================================================-->
  <xs:simpleType name="RiskAnalysisResultCode">
    <xs:annotation>
      <xs:documentation>RISK ANALYSIS.RISK ANALYSIS RESULT.Code (format: an..17)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="1" />
      <xs:maxLength value="17" />
      <xs:pattern value=".{0,4}E" />
      <!-- for upgrade purposes -->
      <xs:pattern value="[!-~]{1,16}E" />
      <!-- for new phases -->
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== StreetNumType                                                             -->
  <!--================================================================================-->
  <xs:simpleType name="StreetNumType">
    <xs:annotation>
      <xs:documentation>Trader Street and Number (format: an..70)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:maxLength value="70" />
      <xs:pattern value=".{1,70}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== StringLatin1                                                              -->
  <!--================================================================================-->
  <xs:simpleType name="StringLatin1">
    <xs:restriction base="xs:normalizedString">
      <xs:pattern value="\p{IsBasicLatin}+" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== TimeType                                                                  -->
  <!--================================================================================-->
  <xs:simpleType name="TimeType">
    <xs:annotation>
      <xs:documentation>The time zone although not included UTC is implied</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:time">
      <xs:pattern value="\d{2}:\d{2}:\d{2}(\.\d+)?" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== TINNewType                                                                -->
  <!--================================================================================-->
  <xs:simpleType name="TINNewType">
    <xs:annotation>
      <xs:documentation>EORI or TCUI Number (format: an..17)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="3" />
      <xs:maxLength value="17" />
      <xs:pattern value="[A-Z]{2}[!-~]{1,15}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== TINType                                                                   -->
  <!--================================================================================-->
  <xs:simpleType name="TINType">
    <xs:annotation>
      <xs:documentation>Trader Identification Number (format:
				an..17)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="StringLatin1">
      <xs:minLength value="3" />
      <xs:maxLength value="17" />
      <xs:pattern value="[A-Z]{2}[^\n\r]*" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== TINTypeRelaxed                                                            -->
  <!--================================================================================-->
  <xs:simpleType name="TINTypeRelaxed">
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="1" />
      <xs:maxLength value="17" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== TIRcarnetNumber                                                           -->
  <!--================================================================================-->
  <xs:simpleType name="TIRcarnetNumber">
    <xs:annotation>
      <xs:documentation>Applicable at 'TIR carnet number' (format: an..12)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="1" />
      <xs:maxLength value="12" />
      <xs:pattern value="([1-9][0-9]{0,6}|(1[0-9]|2[0-4])[0-9]{0,6}|25000000|(X[A-Z]|[A-Z]X)(2[5-9]|[3-9][0-9]|[1-9][0-9][0-9])[0-9]{6})" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== TradNameType                                                              -->
  <!--================================================================================-->
  <xs:simpleType name="TradNameType">
    <xs:annotation>
      <xs:documentation>Trader Name (format: an..70)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:maxLength value="70" />
      <xs:pattern value=".{1,70}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== UCRReferenceNumber35                                                      -->
  <!--================================================================================-->
  <xs:simpleType name="UCRReferenceNumber35">
    <xs:annotation>
      <xs:documentation>UCR Reference Number (format: an..35)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="1" />
      <xs:maxLength value="35" />
      <xs:pattern value="[ -~]{1,35}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== UCRReferenceNumber35_NoSpaces                                             -->
  <!--================================================================================-->
  <xs:simpleType name="UCRReferenceNumber35_NoSpaces">
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="1" />
      <xs:maxLength value="35" />
      <xs:pattern value="[!-~][ -~]{1,33}[!-~]|[!-~]{1,2}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== UCRReferenceNumber70                                                      -->
  <!--================================================================================-->
  <xs:simpleType name="UCRReferenceNumber70">
    <xs:annotation>
      <xs:documentation>UCR Reference Number (format: an..70)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="1" />
      <xs:maxLength value="70" />
      <xs:pattern value="[ -~]{1,70}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== YearType                                                                  -->
  <!--================================================================================-->
  <xs:simpleType name="YearType">
    <xs:annotation>
      <xs:documentation>Format for Year (format: YYYY)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="NumType">
      <xs:length value="4" />
      <xs:pattern value="[2][0][1-9][0-9]" />
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
