    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== DecType                                                                         -->
  <!--================================================================================-->
  <xs:simpleType name="DecType">
    <xs:annotation>
      <xs:documentation>Base class for all n..N,M types</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal" />
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== emailAddressType                                                                -->
  <!--================================================================================-->
  <xs:simpleType name="emailAddressType">
    <xs:restriction base="StringLatin1">
      <xs:maxLength value="70" />
      <xs:pattern value="[^@]+@[^\.]+\..+" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== EmailAddressType256                                                             -->
  <!--================================================================================-->
  <xs:simpleType name="EmailAddressType256">
    <xs:restriction base="StringLatin1">
      <xs:maxLength value="256" />
      <xs:pattern value="[^@]+@[^\.]+\..+" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== EmailAddressType256_NoSpaces                                                    -->
  <!--================================================================================-->
  <xs:simpleType name="EmailAddressType256_NoSpaces">
    <xs:restriction base="StringLatin1">
      <xs:maxLength value="256" />
      <xs:pattern value="\P{Z}[^@]*@[^\.]+\..*\P{Z}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== GNSSLatitude                                                                    -->
  <!--================================================================================-->
  <xs:simpleType name="GNSSLatitude">
    <xs:annotation>
      <xs:documentation>GNSS Latitude (format: an..17)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:pattern value="[+-]?([0-8]?[0-9]\.[0-9]{5,7}|90.000000?0?)" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== GNSSLongitude                                                                   -->
  <!--================================================================================-->
  <xs:simpleType name="GNSSLongitude">
    <xs:annotation>
      <xs:documentation>GNSS Longitude (format: an..17)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:pattern value="[+-]?((0?[0-9]?|1[0-7])[0-9]\.[0-9]{5,7}|180.000000?0?)" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== HolderOfAuthorisationContentType01_NoSpaces                                     -->
  <!--================================================================================-->
  <xs:simpleType name="HolderOfAuthorisationContentType01_NoSpaces">
    <xs:restriction base="xs:normalizedString">
      <xs:pattern value="\P{Z}.{1,15}\P{Z}|\P{Z}{1,2}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== IdentificationNumber35                                                          -->
  <!--================================================================================-->
  <xs:simpleType name="IdentificationNumber35">
    <xs:annotation>
      <xs:documentation>Applicable at 'LOCATION OF GOODS' (format: an..35)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="1" />
      <xs:maxLength value="35" />
      <xs:pattern value="[ -~]{1,35}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== LanguageCodeType                                                                -->
  <!--================================================================================-->
  <xs:simpleType name="LanguageCodeType">
    <xs:annotation>
      <xs:documentation>Language Code (format: a2)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaType">
      <xs:length value="2" />
      <xs:pattern value="[A-Za-z]{2}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== MRNType                                                                         -->
  <!--================================================================================-->
  <xs:simpleType name="MRNType">
    <xs:annotation>
      <xs:documentation>MRN (format: an18), (alias: MRNType)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:pattern value="([0-1][0-9]|[2][0-4])[A-Z]{2}[A-Z0-9]{13}[0-9]" />
      <!-- ================================================================== -->
      <!-- 'Legacy MRN structure - - - - - - - - - - - - - - - - - - - - - - -->
      <!-- ================================================================== -->
      <!-- Definition of the structure of the MRN produced by an ECS-P2 - - - -->
      <!-- application for an ECS-P2 movement (i.e. initiated during the  - - -->
      <!-- Transitional Period) - - - - - - - - - - - - - - - - - - - - - - - -->
      <!-- Until the end of 2023 (i.e. at least during the Transitional - - - -->
      <!-- Period, the AES-P1 applications will use this pattern to validate- -->
      <!-- any MRN received (i.e. MRN received for movements initiated by - - -->
      <!-- an ECS-P2 applications and  MRN received for movements initiated - -->
      <!-- by an AES application) - - - - - - - - -->
      <!-- ================================================================== -->
      <xs:pattern value="([2][4-9]|[3-9][0-9])[A-Z]{2}[A-Z0-9]{12}[A-E][0-9]" />
      <!-- ================================================================== -->
      <!-- 'To Be MRN structure (AES-P1)- - - - - - - - - - - - - - - - - -  -->
      <!-- ================================================================== -->
      <!-- Definition of the structure of the MRN produced by an AES-P1 - - - -->
      <!-- application for AES-P1 movements initiated *after* 01.01.2024 - -  -->
      <!-- (considering the date of the end of Transitional Period in 2023)-  -->
      <!-- From 01.01.2024, the AES-P1 applications will use this pattern to  -->
      <!-- validate any MRN *received*  - - - - - - - - - - - - - - - - - - - -->
      <!-- The structure of the MRN *produced* by the AES-P1 applications - - -->
      <!-- will be  "([2][1-9]|[3-9][0-9])[A-Z]{2}[A-Z0-9]{12}[A-E][0-9]" - - -->
      <!-- for the first NECAs starting AES operations in 2021. - - - - - - - -->
      <!-- ================================================================== -->
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NCTS-P5_DeclarationGoodsItemNumberType                                          -->
  <!--================================================================================-->
  <xs:simpleType name="NCTS-P5_DeclarationGoodsItemNumberType">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="1999" />
      <xs:pattern value="[1-9][0-9]{0,2}|[1][0-9]{3}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NCTS-P5_MRNType                                                                 -->
  <!--================================================================================-->
  <xs:simpleType name="NCTS-P5_MRNType">
    <xs:annotation>
      <xs:documentation>MRN (format: an18), (alias: MRNType)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:pattern value="([0-1][0-9]|[2][0-4])[A-Z]{2}[A-Z0-9]{13}[0-9]" />
      <!-- ================================================================== -->
      <!-- 'Legacy MRN structure - - - - - - - - - - - - - - - - - - - - - - -->
      <!-- ================================================================== -->
      <!-- Definition of the structure of the MRN produced by an NCTS-P4 - -  -->
      <!-- application for an NCTS-P4 movement (i.e. initiated during the - - -->
      <!-- Transitional Period) - - - - - - - - - - - - - - - - - - - - - - - -->
      <!-- Until the end of 2024, the NCTS-P5 applications will use this - -  -->
      <!-- pattern to validate any MRN received (i.e. MRN received for - - -  -->
      <!-- movements initiated by an NCTS-P4 application and  MRN received  - -->
      <!-- for movements initiated by an NCTS-P5 application) - - - - - - - - -->
      <!-- ================================================================== -->
      <xs:pattern value="([2][4-9]|[3-9][0-9])[A-Z]{2}[A-Z0-9]{12}[J-M][0-9]" />
      <!-- ================================================================== -->
      <!-- 'To Be MRN structure (NCTS-P5)- - - - - - - - - - - - - - - - - - -->
      <!-- ================================================================== -->
      <!-- Definition of the structure of the MRN produced by an NCTS-P5 - -  -->
      <!-- application for NCTS-P5 movements initiated *after* 01.01.2024 - - -->
      <!-- (considering the date of the end of Transitional Period in 2023)-  -->
      <!-- From 01.01.2024, the NCTS-P5 applications will use this pattern to -->
      <!-- validate any MRN *received*  - - - - - - - - - - - - - - - - - - - -->
      <!-- The structure of the MRN *produced* by the NCTS-P5 applications -  -->
      <!-- will be  "([2][1-9]|[3-9][0-9])[A-Z]{2}[A-Z0-9]{12}[J-M][0-9]" - - -->
      <!-- for the first NTAs starting Phase 5 operations in 2021. - - - - -  -->
      <!-- ================================================================== -->
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithoutZero_3                                                            -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithoutZero_3">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="999" />
      <xs:pattern value="[1-9][0-9]{0,2}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithoutZero_5                                                            -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithoutZero_5">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="99999" />
      <xs:pattern value="[1-9][0-9]{0,4}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithoutZero_8                                                            -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithoutZero_8">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="99999999" />
      <xs:pattern value="[1-9][0-9]{0,7}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithZero_4                                                               -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithZero_4">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="9999" />
      <xs:pattern value="(0|[1-9][0-9]{0,3})" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithZero_8                                                               -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithZero_8">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="99999999" />
      <xs:pattern value="(0|[1-9][0-9]{0,7})" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumericWithZero_9                                                               -->
  <!--================================================================================-->
  <xs:simpleType name="NumericWithZero_9">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="999999999" />
      <xs:pattern value="(0|[1-9][0-9]{0,8})" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== NumType                                                                         -->
  <!--================================================================================-->
  <xs:simpleType name="NumType">
    <xs:annotation>
      <xs:documentation>Base class for all nN and n..N
				types</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token" />
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== phaseIDtype                                                                     -->
  <!--================================================================================-->
  <xs:simpleType name="phaseIDtype">
    <xs:restriction base="xs:token">
      <xs:enumeration value="AES1.1">
        <xs:annotation>
          <xs:documentation>Phase AES 1.1</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="AES1.0">
        <xs:annotation>
          <xs:documentation>Phase AES 1.0</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== PostalCodeType                                                                  -->
  <!--================================================================================-->
  <xs:simpleType name="PostalCodeType">
    <xs:annotation>
      <xs:documentation>Trader Postal Code (format: an..17)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:maxLength value="17" />
      <xs:pattern value=".{1,17}" />
    </xs:restriction>
    <!-- Updated -->
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== regionOfDispatch                                                                -->
  <!--================================================================================-->
  <xs:simpleType name="regionOfDispatch">
    <xs:annotation>
      <xs:documentation>Region of dispatch (format: an..9)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="1" />
      <xs:maxLength value="9" />
      <xs:pattern value="[!-~]{1,9}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== RiskAnalysisResultCode                                                          -->
  <!--================================================================================-->
  <xs:simpleType name="RiskAnalysisResultCode">
    <xs:annotation>
      <xs:documentation>RISK ANALYSIS.RISK ANALYSIS RESULT.Code (format: an..17)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="1" />
      <xs:maxLength value="17" />
      <xs:pattern value=".{0,4}[DEZ]" />
      <!-- for upgrade purposes -->
      <xs:pattern value="[!-~]{1,16}E" />
      <!-- for new phases -->
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== StreetNumType                                                                   -->
  <!--================================================================================-->
  <xs:simpleType name="StreetNumType">
    <xs:annotation>
      <xs:documentation>Trader Street and Number (format: an..70)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:maxLength value="70" />
      <xs:pattern value=".{1,70}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== StringLatin1                                                                    -->
  <!--================================================================================-->
  <xs:simpleType name="StringLatin1">
    <xs:restriction base="xs:normalizedString">
      <xs:pattern value="\p{IsBasicLatin}+" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== TINNewType                                                                      -->
  <!--================================================================================-->
  <xs:simpleType name="TINNewType">
    <xs:annotation>
      <xs:documentation>EORI or TCUI Number (format: an..17)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="3" />
      <xs:maxLength value="17" />
      <xs:pattern value="[A-Z]{2}[!-~]{1,15}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== TINType                                                                         -->
  <!--================================================================================-->
  <xs:simpleType name="TINType">
    <xs:annotation>
      <xs:documentation>Trader Identification Number (format:
				an..17)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="StringLatin1">
      <xs:minLength value="3" />
      <xs:maxLength value="17" />
      <xs:pattern value="[A-Z]{2}[^\n\r]*" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== TIRcarnetNumber                                                                 -->
  <!--================================================================================-->
  <xs:simpleType name="TIRcarnetNumber">
    <xs:annotation>
      <xs:documentation>Applicable at 'TIR carnet number' (format: an..12)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="1" />
      <xs:maxLength value="12" />
      <xs:pattern value="([1-9][0-9]{0,6}|(1[0-9]|2[0-4])[0-9]{0,6}|25000000|(X[A-Z]|[A-Z]X)(2[5-9]|[3-9][0-9]|[1-9][0-9][0-9])[0-9]{6})" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== TradNameType                                                                    -->
  <!--================================================================================-->
  <xs:simpleType name="TradNameType">
    <xs:annotation>
      <xs:documentation>Trader Name (format: an..70)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:maxLength value="70" />
      <xs:pattern value=".{1,70}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== UCRReferenceNumber35                                                            -->
  <!--================================================================================-->
  <xs:simpleType name="UCRReferenceNumber35">
    <xs:annotation>
      <xs:documentation>UCR Reference Number (format: an..35)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="1" />
      <xs:maxLength value="35" />
      <xs:pattern value="[ -~]{1,35}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== UCRReferenceNumber35_NoSpaces                                                   -->
  <!--================================================================================-->
  <xs:simpleType name="UCRReferenceNumber35_NoSpaces">
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="1" />
      <xs:maxLength value="35" />
      <xs:pattern value="[!-~][ -~]{1,33}[!-~]|[!-~]{1,2}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== UCRReferenceNumber70                                                            -->
  <!--================================================================================-->
  <xs:simpleType name="UCRReferenceNumber70">
    <xs:annotation>
      <xs:documentation>UCR Reference Number (format: an..70)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="AlphaNumType">
      <xs:minLength value="1" />
      <xs:maxLength value="70" />
      <xs:pattern value="[ -~]{1,70}" />
    </xs:restriction>
  </xs:simpleType>
  <!--================================================================================-->
  <!--===== YearType                                                                        -->
  <!--================================================================================-->
  <xs:simpleType name="YearType">
    <xs:annotation>
      <xs:documentation>Format for Year (format: YYYY)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="NumType">
      <xs:length value="4" />
      <xs:pattern value="[2][0][1-9][0-9]" />
    </xs:restriction>
  </xs:simpleType>
</xs:schema>
